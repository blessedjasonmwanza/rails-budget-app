<div id="<%= dom_id transaction %>" class="transaction">
  <span><%= transaction.name%></span>
  <span><%= transaction.created_at.strftime("%e %B %Y") %></span>
  <span><%= number_to_currency(transaction.amount) %></span>
  <%= button_to 'detele', category_transaction_path(id: transaction.id), method: :delete,  data: {turbo: false}, class: 'btn bg-danger' %>
</div>
